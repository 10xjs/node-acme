{
  "env": {
    "node": true
  },
  "rules": {
    "array-bracket-spacing": [2, "never"],
    "block-spacing": [2, "always"],
    "comma-spacing": [2, {before: false, after: true}],
    "consistent-return": [2],
    "curly": [2],
    "default-case": [2],
    "dot-location": [2, "property"],
    "eol-last": [2, "unix"],
    "eqeqeq": [1, "smart"],
    "guard-for-in": [2],
    "handle-callback-err": [2],
    "indent": [1, 2, {VariableDeclarator: {var: 2}, SwitchCase: 1}],
    "key-spacing": [1, {align: "value", beforeColon: false, afterColon: true}],
    "max-params": [1],
    "no-cond-assign": [2],
    "no-duplicate-case": [2],
    "no-else-return": [2],
    "no-extra-semi": [2],
    "no-fallthrough": [2],
    "no-mixed-requires": [2],
    "no-shadow": [1],
    "no-throw-literal": [2],
    "no-trailing-spaces": [2],
    "no-unreachable": [2],
    "no-unused-vars": [2],
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "space-before-function-paren": [1, "never"],
    "strict": [2, "global"],
  }
}
